<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="style.uss" />
    <ui:VisualElement name="popupscontainer" style="width: 100%; height: 100%; align-items: center; justify-content: center;">
        <ui:VisualElement name="ventanapopup" style="width: 40%; height: 80%;" />
    </ui:VisualElement>
    <ui:VisualElement name="Container" class="principalcontainer" style="position: absolute;">
        <ui:VisualElement name="UserContainer" style="align-items: flex-end; height: 15%; width: 100%;">
            <ui:Button text="Button" display-tooltip-when-elided="true" name="UserButton" />
        </ui:VisualElement>
        <ui:VisualElement name="TituloContainer" class="titulocontainer">
            <ui:Label text="CARLOS EL BOMBAS" display-tooltip-when-elided="true" name="TituloText" class="titulo" />
        </ui:VisualElement>
        <ui:VisualElement name="TextButtons" style="height: 20%; width: 100%; flex-direction: row; justify-content: center; align-items: center;">
            <ui:Button text="Button" display-tooltip-when-elided="true" name="Individual" class="test" />
            <ui:Button text="Button" display-tooltip-when-elided="true" name="Multijugador" class="test" />
        </ui:VisualElement>
        <ui:VisualElement name="IconButtons" style="align-items: center; white-space: nowrap; width: 100%; height: 20%; flex-direction: row; justify-content: center;">
            <ui:Button text="Button" display-tooltip-when-elided="true" name="clasif" style="white-space: normal;" />
            <ui:Button text="Button" display-tooltip-when-elided="true" name="skin" />
            <ui:Button text="Button" display-tooltip-when-elided="true" name="controles" />
            <ui:Button text="Button" display-tooltip-when-elided="true" name="ajustes" />
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
